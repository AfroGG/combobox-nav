{
  "plugins": ["github"],
  "extends": [
    "plugin:github/browser",
    "plugin:github/recommended",
    "plugin:github/typescript"
  ],
  "globals": {
    "Combobox": "readable"
  },
  "overrides": [
    {
      "files": "test/**/*.js",
      "rules": {
        "github/no-inner-html": "off",
        "github/unescaped-html-literal": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off"
      }
    }
  ]
}
